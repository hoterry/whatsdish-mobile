{
  "expo": {
    "name": "Whatsdish",
    "slug": "whatsdish-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true,
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "We need your location to show you the distance to restaurants nearby.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We need your location to show you the distance to restaurants nearby.",
        "NSLocationAlwaysUsageDescription": "We need your location to show you the distance to restaurants nearby.",
        "aps-environment": "development"
      },
      "bundleIdentifier": "com.whatsdish.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "statusBar": {
        "backgroundColor": "transparent",
        "translucent": true,
        "hidden": false,
        "barStyle": "dark-content"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "We need your location to show you the distance to restaurants nearby."
        }
      ],
      [
        "expo-notifications"
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "afe593c8-471a-4e86-b604-bccf9533623e"
      }
    },
    "owner": "whatsdish_app"
  }
}